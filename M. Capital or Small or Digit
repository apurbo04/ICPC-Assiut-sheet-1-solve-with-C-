#include<stdio.h>
 
int main()
{
    char x;
    scanf("%c",&x);
 
    if (47<x && x<58 )
    {
        printf("IS DIGIT");
    }
    
    else if (64<x && x<91)
    {
        printf("ALPHA\nIS CAPITAL");
    }
    
    else if (96<x && x<123 )
    {
        printf("ALPHA\nIS SMALL");
    }
    
    return 0;
}
