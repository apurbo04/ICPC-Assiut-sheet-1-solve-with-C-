#include <stdio.h>
 
int main()
{
    long long a, b;
    if (1 <= a && b <= 10 ^ 5)
    {
        scanf("%lld %lld", &a, &b);
        printf("%lld + %lld = %lld\n", a, b, a + b);
        printf("%lld * %lld = %lld\n", a, b, a * b);
        printf("%lld - %lld = %lld\n", a, b, a - b);
    }
    
    return 0;
}
